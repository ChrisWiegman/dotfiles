{
    "[go]": {
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit"
        },
        "editor.formatOnSave": true
    },
    "[php]": {
        "editor.defaultFormatter": "wongjn.php-sniffer"
    },
    "[rust]": {
        "editor.formatOnSave": true
    },
    "cSpell.userWords": [
        "Wiegman"
    ],
    "chat.commandCenter.enabled": false,
    "debug.console.fontFamily": "MonoLisa",
    "debug.console.fontSize": 13,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "editor.cursorBlinking": "smooth",
    "editor.cursorStyle": "block",
    "editor.fontFamily": "MonoLisa",
    "editor.fontLigatures": "'calt' on, 'liga' on, 'zero' on, 'ss02' on",
    "editor.fontSize": 13,
    "editor.fontVariations": false,
    "editor.formatOnPaste": false,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.guides.bracketPairs": "active",
    "editor.guides.bracketPairsHorizontal": false,
    "editor.hover.delay": 250,
    "editor.inlayHints.fontSize": 11,
    "editor.inlayHints.padding": true,
    "editor.largeFileOptimizations": false,
    "editor.lineHeight": 25,
    "editor.minimap.enabled": false,
    "editor.rulers": [
        120
    ],
    "editor.semanticHighlighting.enabled": true,
    "editor.smoothScrolling": true,
    "emmet.includeLanguages": {
        "javascript": "javascriptreact",
        "typescript": "html"
    },
    "emmet.triggerExpansionOnTab": true,
    "eslint.validate": [
        "typescript",
        "typescriptreact",
        "javascript",
        "javascriptreact"
    ],
    "everforest.italicComments": true,
    "everforest.italicKeywords": true,
    "explorer.compactFolders": false,
    "explorer.confirmDelete": false,
    "explorer.decorations.badges": false,
    "explorer.openEditors.visible": 1,
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 500,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "git-graph.dialog.addTag.pushToRemote": true,
    "git-graph.dialog.merge.noFastForward": false,
    "git-graph.repository.commits.fetchAvatars": true,
    "git-graph.repository.commits.loadMore": 300,
    "git-graph.repository.sign.commits": true,
    "git-graph.repository.sign.tags": true,
    "gitblame.inlineMessageEnabled": true,
    "github.copilot.editor.enableAutoCompletions": true,
    "githubPullRequests.createOnPublishBranch": "ask",
    "githubPullRequests.pullBranch": "prompt",
    "go.inlayHints.assignVariableTypes": true,
    "go.inlayHints.compositeLiteralFields": true,
    "go.inlayHints.compositeLiteralTypes": true,
    "go.inlayHints.constantValues": true,
    "go.inlayHints.functionTypeParameters": true,
    "go.inlayHints.parameterNames": true,
    "go.inlayHints.rangeVariableTypes": true,
    "go.lintFlags": [
        "--fast"
    ],
    "go.lintOnSave": "workspace",
    "go.lintTool": "golangci-lint",
    "go.toolsManagement.autoUpdate": true,
    "gopls": {
        "ui.noSemanticNumber": true,
        "ui.noSemanticString": true,
        "ui.semanticTokens": true
    },
    "intelephense.codeLens.implementations.enable": true,
    "intelephense.codeLens.overrides.enable": true,
    "intelephense.codeLens.parent.enable": true,
    "intelephense.codeLens.references.enable": true,
    "intelephense.codeLens.usages.enable": true,
    "intelephense.stubs": [
        "apache",
        "bcmath",
        "bz2",
        "calendar",
        "com_dotnet",
        "Core",
        "ctype",
        "curl",
        "date",
        "dba",
        "dom",
        "enchant",
        "exif",
        "FFI",
        "fileinfo",
        "filter",
        "fpm",
        "ftp",
        "gd",
        "gettext",
        "gmp",
        "hash",
        "iconv",
        "imap",
        "intl",
        "json",
        "ldap",
        "libxml",
        "mbstring",
        "meta",
        "mysqli",
        "oci8",
        "odbc",
        "openssl",
        "pcntl",
        "pcre",
        "PDO",
        "pdo_ibm",
        "pdo_mysql",
        "pdo_pgsql",
        "pdo_sqlite",
        "pgsql",
        "Phar",
        "posix",
        "pspell",
        "random",
        "readline",
        "Reflection",
        "session",
        "shmop",
        "SimpleXML",
        "snmp",
        "soap",
        "sockets",
        "sodium",
        "SPL",
        "sqlite3",
        "standard",
        "superglobals",
        "sysvmsg",
        "sysvsem",
        "sysvshm",
        "tidy",
        "tokenizer",
        "xml",
        "xmlreader",
        "xmlrpc",
        "xmlwriter",
        "xsl",
        "Zend OPcache",
        "zip",
        "zlib",
        "wordpress"
    ],
    "intelephense.telemetry.enabled": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "laravel-pint.enable": true,
    "phpParameterHint.hintOnlyLine": false,
    "phpSniffer.autoDetect": true,
    "remoteHub.gitProtocol": "ssh",
    "remoteHub.richNavigation.enabled": true,
    "security.workspace.trust.untrustedFiles": "open",
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.confirmOnExit": "never",
    "terminal.integrated.fontFamily": "MonoLisa",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.lineHeight": 1.4,
    "typescript.surveys.enabled": false,
    "update.mode": "none",
    "update.showReleaseNotes": false,
    "window.closeWhenEmpty": true,
    "window.newWindowDimensions": "inherit",
    "window.openFilesInNewWindow": "default",
    "window.restoreWindows": "none",
    "window.title": "${remoteName}${separator}${activeRepositoryBranchName}${separator}${activeEditorShort}${separator}${rootName}${separator}${profileName}",
    "window.titleBarStyle": "custom",
    "workbench.colorTheme": "Everforest Dark",
    "workbench.commandPalette.history": 10,
    "workbench.enableExperiments": false,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.settings.editor": "json",
    "workbench.settings.useSplitJSON": true,
    "workbench.startupEditor": "none",
    "workbench.tree.indent": 20
}